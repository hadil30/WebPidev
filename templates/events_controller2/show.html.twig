<!DOCTYPE html>

<title>Events</title>

{% block body %}
    <h1>Events</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>EventId</th>
                <td>{{ event.eventId }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ event.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ event.description }}</td>
            </tr>
            <tr>
                <th>Typee</th>
                <td>{{ event.typee }}</td>
            </tr>
            <tr>
                <th>Datee</th>
                <td>{{ event.datee ? event.datee|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ event.status }}</td>
            </tr>
            <tr>
                <th>ImageUrl</th>
                <td>{{ event.imageUrl }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_events_controller2_index') }}">back to list</a>

    <a href="{{ path('app_events_controller2_edit', {'eventId': event.eventId}) }}">edit</a>

    {{ include('events_controller2/_delete_form.html.twig') }}
{% endblock %}
